{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": true,
    "no-unused-expression": [false],
    "interface-name": false,
    "no-console": false,
    "only-arrow-functions": false,
    "no-implicit-dependencies": false,
    "no-submodule-imports": [false],
    "jsx-wrap-multiline": false
  }
}